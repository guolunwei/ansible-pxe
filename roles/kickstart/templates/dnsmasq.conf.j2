interface=ens33
bind-interfaces
domain=ericsson.com

dhcp-range=10.0.0.100,10.0.0.240,255.255.255.0,12h
dhcp-option=option:router,10.0.0.1
dhcp-option=option:dns-server,114.114.114.114
dhcp-option=option:dns-server,8.8.8.8

dhcp-boot=pxelinux.0
pxe-prompt="Press F8 for menu.", 10
pxe-service=x86PC, "Install {{ os_name }}", pxelinux

enable-tftp
tftp-root="{{ tftp_root }}"
